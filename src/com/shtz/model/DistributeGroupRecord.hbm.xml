<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_distributeGroupRecord" name="com.shtz.model.DistributeGroupRecord">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <property name="planIds" access="field"/>
    <property name="pcIds" access="field"/>
    <property name="type" access="field"/>
    <property name="addDate" access="field"/>
    <many-to-one access="field" name="distributor" />
  </class>
</hibernate-mapping>
