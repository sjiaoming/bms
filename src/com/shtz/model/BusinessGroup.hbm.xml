<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_businessGroup" name="com.shtz.model.BusinessGroup">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <property name="groupName" access="field"/>
    <property name="groupNum" access="field"/>
    <property name="groupType" access="field"/>
    <many-to-one access="field" name="groupLeader" />
  </class>
</hibernate-mapping>
